div.starter-template
    h1 {{proposal.title}}
    h4 {{proposal.description}}
    br
    p Fill out the form below to get a price
    br
    div(ng-repeat='question in proposal.questions')
        h3 {{question.title}}
        ul(ng-repeat='option in question.qOptions')
            li.no-decoration: input(type='radio', name='{{question.qId}}', value='{{option.optionValue}}')span  {{option.optionChoice}}




        br






